{
  "manifest_version": 2,
  "name": "removeit",
  "version": "0.0.1",
  "description": "Remove html elements from your browser.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.1.min.js", "jquery-ui.min.js", "content.js"]
    }
  ],
  "permissions": ["storage"]
}
