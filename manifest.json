{
  "manifest_version": 2,
  "name": "removeit",
  "version": "0.0.5",
  "description": "Remove html elements from your browser.",
  "icons": { "128": "remove_it_logo.png" },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.1.min.js", "jquery-ui.min.js", "content.js"]
    }
  ],
  "permissions": ["storage", "tabs", "activeTab", "webNavigation"]
}
